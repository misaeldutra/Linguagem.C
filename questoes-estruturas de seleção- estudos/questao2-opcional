#include <stdio.h>
#include <stdlib.h>

//Faça um algoritmo para ler a idade de uma pessoa e exibir uma mensagem dizendo se ela está compreendida entre 18 e 65 anos.

int main(){
    int idade;

printf("Digite sua idade: ");
scanf("%d", &idade);

if(idade>= 18 && idade<=65){
    printf("A sua idade está compreendida entre 18 e 65 anos! \n");
}
system("pause");
return(0);

}